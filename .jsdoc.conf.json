{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"default":{
			"includeDate": false,
			"layoutFile": "./.jsdoc.layout.tmpl",
			"staticFiles": {
				"include": ["./docs/img"],
				"includePattern" : "logo-xhr*"
			}
		},
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
